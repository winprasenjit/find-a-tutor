{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "project": {
        "name": "ng-cli"
    },
    "apps": [
        {
            "root": "src",
            "outDir": "dist",
            "assets": [
                "assets",
                "favicon.ico",
                "app/shared/json"
            ],
            "index": "index.html",
            "main": "main.ts",
            "polyfills": "polyfills.ts",
            "test": "test.ts",
            "tsconfig": "tsconfig.app.json",
            "testTsconfig": "tsconfig.spec.json",
            "prefix": "app",
            "styles": [
                "../node_modules/bootstrap/dist/css/bootstrap.min.css",
                "../node_modules/gridstack/dist/gridstack.min.css",
                "../node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
                "assets/vendors/font-awesome/css/font-awesome.css",
                "assets/vendors/sb-admin-2/sb-admin-2.css",
                "assets/vendors/metisMenu/metisMenu.css",
                "assets/vendors/material/material-icon.css",
                "assets/css/styles.css"
            ],
            "scripts": [
                "../node_modules/jquery/dist/jquery.min.js",
                "../node_modules/lodash/lodash.js",
                "../node_modules/gridstack/dist/gridstack.min.js",
                "../node_modules/bootstrap/dist/js/bootstrap.min.js",
                "assets/vendors/sb-admin-2/sb-admin-2.js",
                "assets/vendors/metisMenu/metisMenu.js"
            ],
            "environmentSource": "environments/environment.ts",
            "environments": {
                "dev": "environments/environment.ts",
                "prod": "environments/environment.prod.ts"
            }
        }
    ],
    "e2e": {
        "protractor": {
            "config": "./protractor.conf.js"
        }
    },
    "lint": [
        {
            "project": "src/tsconfig.app.json"
        },
        {
            "project": "src/tsconfig.spec.json"
        },
        {
            "project": "e2e/tsconfig.e2e.json"
        }
    ],
    "test": {
        "karma": {
            "config": "./karma.conf.js"
        }
    },
    "defaults": {
        "styleExt": "css",
        "component": {}
    }
}